{
  "name": "transactions",
  "scripts": {
    "start": "set -a && source .env && set +a && npx concurrently \"npm:client:start\" \"npm:server:start\"",
    "client:start": "cd client && npm run start",
    "server:start": "cd server && npm run start",
    "preinstall": "(cd client && npm i) && (cd server && npm i)"
  },
  "devDependencies": {
    "concurrently": "8.0.1"
  }
}