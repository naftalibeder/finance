{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist",
    "lib": ["es2021"],
    "target": "es2016",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  }
}
